# making a function to determine whether the number is prime or not
def isprime(y):
    prime = True
    for x in range(2,y-1):
        if y % x == 0:
            prime = False
            break
    return(prime)


# print all prime numbers from 1 to 100

for z in range(2,100):
    if isprime(z):
        print(z)


